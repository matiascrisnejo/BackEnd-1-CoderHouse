<h1>Tu carrito</h1>
<table>
    <tr>
        <th>Producto</th>
        <th>Precio</th>
        <th>Descripcion</th>
        <th>Cantidad</th>
    </tr>

    {{#each productsDetail}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td>{{this.description}}</td>
            <td>{{this.quantity}}</td>
        </tr>
        {{/each}}
</table>